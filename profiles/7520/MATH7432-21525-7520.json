    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Explain the interplay between a norm and its induced metric and topology on a vector space"
        },
        {
            "number": "LO2.",
            "description": "Describe linear operators and functionals on normed space"
        },
        {
            "number": "LO3.",
            "description": "Discuss the Hahn-Banach theorem and some of its consequences"
        },
        {
            "number": "LO4.",
            "description": "Analyse finite dimensional normed spaces via their unit balls"
        },
        {
            "number": "LO5.",
            "description": "Discuss the Baire category theorem, the uniform boundedness principle, the closed-graph theorem and the open-mapping theorem"
        },
        {
            "number": "LO6.",
            "description": "Describe weak topologies and duality"
        },
        {
            "number": "LO7.",
            "description": "Explain the geometry of Hilbert spaces via orthogonality, projections, the Riesz representation theorem and adjoint operators"
        },
        {
            "number": "LO8.",
            "description": "Examine how ideas from this course arise in other areas of mathematics"
        }
    ],
