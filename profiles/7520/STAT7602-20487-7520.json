    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "derive and compare population measures of mortality, illness, fertility and survival, using basic demographic tools such as life tables and age standardisation"
        },
        {
            "number": "LO2.",
            "description": "access the main sources of routinely collected health data and choose the appropriate one, taking into account their advantages and disadvantages"
        },
        {
            "number": "LO3.",
            "description": "design a valid and reliable health survey to collect primary data"
        },
        {
            "number": "LO4.",
            "description": "choose the most appropriate mode of delivery for a survey"
        },
        {
            "number": "LO5.",
            "description": "design an efficient sampling strategy to obtain a random sample of the target population"
        },
        {
            "number": "LO6.",
            "description": "estimate means, totals and proportions from survey data, taking into account the sample design, and analyse, interpret and present these results"
        }
    ],
