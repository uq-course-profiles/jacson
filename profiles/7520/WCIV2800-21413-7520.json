        {
            "number": "LO1.",
            "description": "Understand and intervene in conceptual contest over the rule of law."
        },
        {
            "number": "LO2.",
            "description": "Understand key episodes of legal contest in the history of the rule of law."
        },
        {
            "number": "LO3.",
            "description": "Understand the role of the rule of law in the context of Western societies over time."
        },
        {
            "number": "LO4.",
            "description": "Understand and critically evaluate key schools of thought regarding the rule of law."
        },
        {
            "number": "LO5.",
            "description": "Appreciate and critically evaluate the relationship between the rule of law and other principles."
        },
        {
            "number": "LO6.",
            "description": "Appreciate and critically evaluate the practical legal and political implications of the rule of law."
        },
        {
            "number": "LO7.",
            "description": "Demonstrate analytical reasoning and scholarly research skills in applying this knowledge to debates regarding the rule of law."
        },
        {
            "number": "LO8.",
            "description": "Demonstrate ability to communicate clearly and persuasively about the rule of law in written and spoken form."
        }
