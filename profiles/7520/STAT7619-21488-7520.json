    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Explain the theory of generalised linear models (GLMs) and statistical inference based on GLMs"
        },
        {
            "number": "LO2.",
            "description": "Analyse data using logistic regression models for binary, multinomial and ordinal categorical data"
        },
        {
            "number": "LO3.",
            "description": "Analyse count and rate data using Poisson regression, Negative Binomial, and continuous data using GLMs"
        },
        {
            "number": "LO4.",
            "description": "Explain the nature of survival data, and summarise and display survival data using nonparametric methods, including the Kaplan-Meier curve"
        },
        {
            "number": "LO5.",
            "description": "Analyse survival data using the Cox proportional hazards regression model, including time-dependent covariates and the stratified Cox model"
        },
        {
            "number": "LO6.",
            "description": "To assess and evaluate the model fit and diagnostics of GLMs and survival models"
        },
        {
            "number": "LO7.",
            "description": "Synthesise results of analyses to present and communicate findings"
        }
    ],
