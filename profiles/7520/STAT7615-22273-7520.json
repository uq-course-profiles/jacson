    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Use counterfactuals (potential outcomes) to precisely define causal effects"
        },
        {
            "number": "LO2.",
            "description": "Describe the differences between association and causation, and the fundamental assumptions required for causation"
        },
        {
            "number": "LO3.",
            "description": "Implement causal inference methods, using software, for single time point and longitudinal exposures, and for mediation analyses"
        },
        {
            "number": "LO4.",
            "description": "Construct causal diagrams and use them to identify potential sources of bias"
        },
        {
            "number": "LO5.",
            "description": "Interpret results of analyses in light of the causal assumptions required"
        },
        {
            "number": "LO6.",
            "description": "Effectively communicate results of causal analyses in language suitable for a clinical or epidemiological journal"
        }
    ],
