    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Recognise the existence of correlated or hierarchical data structures, and describe the limitations of standard methods in these settings;"
        },
        {
            "number": "LO2.",
            "description": "Develop and analytically describe an appropriate model for longitudinal or correlated data based on subject matter considerations;"
        },
        {
            "number": "LO3.",
            "description": "Be proficient at using statistical software packages (Stat & SAS) to properly model and perform computations for longitudinal data analyses, and to correctly interpret results;"
        },
        {
            "number": "LO4.",
            "description": "Express the results of statistical analyses of longitudinal data in language suitable for communication to medical investigators or publication in biomedical or epidemiological journal articles."
        }
    ],
