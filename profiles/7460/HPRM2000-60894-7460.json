    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Define health promotion and its role in wider public health domain"
        },
        {
            "number": "LO2.",
            "description": "Discuss current national health priorities as they relate to physical activity, exercise and nutrition"
        },
        {
            "number": "LO3.",
            "description": "Discuss different approaches to health promotion including the advantages and disadvantages of individual and population based approaches"
        },
        {
            "number": "LO4.",
            "description": "Critique behaviour change and socio-ecological theory and the role of environments in promoting physical activity, exercise and nutrition"
        },
        {
            "number": "LO5.",
            "description": "Critically understand the emerging role apps, wearables and social media play in the promotion of physical activity, exercise and nutrition."
        },
        {
            "number": "LO6.",
            "description": "Discuss the processes used to design evidence based health promotion interventions"
        },
        {
            "number": "LO7.",
            "description": "Discuss methods and approaches used to evaluate health promotion programs"
        }
    ],
