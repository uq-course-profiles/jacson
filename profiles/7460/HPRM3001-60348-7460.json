        {
            "number": "LO1.",
            "description": "Consider a range of behaviour change strategies applied to physical activity and health in specific age groups, and how these strategies are used to promote active lifestyles."
        },
        {
            "number": "LO2.",
            "description": "Critically appraise physical activity promotion research and practice, and identify how research is used to inform practice."
        },
        {
            "number": "LO3.",
            "description": "Critically review an existing physical activity promotion strategy"
        },
        {
            "number": "LO4.",
            "description": "Develop a new physical activity initiative for a specific population group."
        },
        {
            "number": "LO5.",
            "description": "Design a comprehensive evaluation plan for a new physical activity promotion strategy"
        },
        {
            "number": "LO6.",
            "description": "Consider ethical issues relating to research and practice in physical activity promotion"
        },
        {
            "number": "LO7.",
            "description": "Work in an interdisciplinary team"
        }
