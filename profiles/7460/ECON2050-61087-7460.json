    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Demonstrate a clear understanding of basic topological concepts in Euclidean spaces."
        },
        {
            "number": "LO2.",
            "description": "Apply key concepts from the calculus of several variables such as directional and total derivatives."
        },
        {
            "number": "LO3.",
            "description": "Identify solutions of some unconstrained optimization problems."
        },
        {
            "number": "LO4.",
            "description": "Identify solutions of some problems of constrained optimization with equality and with inequality contraints."
        },
        {
            "number": "LO5.",
            "description": "Apply constrained and unconstrained optimization techniques to various economic questions."
        },
        {
            "number": "LO6.",
            "description": "Apply dynamic programming in discrete time settings."
        }
    ],
