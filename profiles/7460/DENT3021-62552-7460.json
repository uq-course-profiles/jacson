        {
            "number": "LO1.",
            "description": "Apply basic oral epidemiology terminology, definitions and calculations using manual and technology-assisted methods"
        },
        {
            "number": "LO2.",
            "description": "Critically review and synthesise existing evidence from a range of sources to develop coherent and logical arguments"
        },
        {
            "number": "LO3.",
            "description": "Produce informed and original research questions and hypotheses for a health-based context"
        },
        {
            "number": "LO4.",
            "description": "Compare and contrast study designs used to answer clinical questions in oral health research"
        },
        {
            "number": "LO5.",
            "description": "Interpret and professionally communicate evidence-based practice knowledge, skills and ideas to expert audiences"
        }
