    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Understand the value of dermatoscopy as an integral part of the clinical assessment of pigmented and non-pigmented skin tumours."
        },
        {
            "number": "LO2.",
            "description": "Confidently describe the key dermatoscopy features of skin lesions using descriptive/Kittlerian terminology."
        },
        {
            "number": "LO3.",
            "description": "Confidently and safely identify suspicious skin lesions requiring biopsy using the 'Chaos and Clues' algorithm for pigmented lesions and 'Prediction Without Pigment' algorithm for non-pigmented lesions."
        },
        {
            "number": "LO4.",
            "description": "Confidently make lesion-specific diagnosis for a wide range of skin lesions either by pattern recognition or by formal pattern analysis."
        },
        {
            "number": "LO5.",
            "description": "Confidently assess, diagnose and manage the range of skin lesions which commonly present to skin cancer practitioners."
        },
        {
            "number": "LO6.",
            "description": "Make predictably safe initial management decisions for the less common and rare skin lesions which may present to skin cancer practitioners."
        },
        {
            "number": "LO7.",
            "description": "Demonstrate understanding of basic statistical concepts relevant to appraising journal articles on skin cancer."
        }
    ],
