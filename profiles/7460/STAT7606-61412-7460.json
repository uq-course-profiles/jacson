    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Identify the benefits of randomisation as a mechanism for reducing bias, and implement a variety of randomisation schemes"
        },
        {
            "number": "LO2.",
            "description": "Demonstrate knowledge of the principles behind the common experimental designs"
        },
        {
            "number": "LO3.",
            "description": "Describe the efficiency advantages of crossover designs, and be able to design and interpret the two-period crossover study"
        },
        {
            "number": "LO4.",
            "description": "Demonstrate an understanding of the principles underlying Phase I, II, III and IV studies, as well as an appreciation of the scientific basis underlying issues in clinical studies including intention-to-treat, blinding, interim analyses and subgroup analyses"
        },
        {
            "number": "LO5.",
            "description": "Appreciate the importance of sample size in clinical studies, and perform sample size calculations for a variety of trial designs with different outcomes"
        }
    ],
