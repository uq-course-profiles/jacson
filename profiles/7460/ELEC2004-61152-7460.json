    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Explain and be able to apply linear circuit theorems and circuit analysis methods such as mesh current and node voltage"
        },
        {
            "number": "LO2.",
            "description": "Explain operating principle of operational amplifiers and be able to build and analyse the circuits containing operational amplifiers"
        },
        {
            "number": "LO3.",
            "description": "To be able to evaluate and analyse the time responses of switching circuits containing a resistor, a single capacitor and/or an inductor"
        },
        {
            "number": "LO4.",
            "description": "Analyse the response of a resistor / inductor /capacitor circuits to an AC excitation of a single frequency"
        },
        {
            "number": "LO5.",
            "description": "Analyse the power in resistor / inductor /capacitor circuits excited by a single frequency of AC"
        },
        {
            "number": "LO6.",
            "description": "Intuitively generate the frequency response (Bode plot) of a known circuit and be able to interpret the frequency response (Bode plot) of an unknown circuit"
        },
        {
            "number": "LO7.",
            "description": "Build and analyse electronic circuits with resistors, capacitors and inductors"
        },
        {
            "number": "LO8.",
            "description": "Be able to effectively use Laplace and Fourier transforms in circuit analysis"
        },
        {
            "number": "LO9.",
            "description": "Explain the concept of transfer functions and be able to use them in circuit analysis"
        },
        {
            "number": "LO10.",
            "description": "Design circuits to generate transfer functions"
        },
        {
            "number": "LO11.",
            "description": "Build and verify electronic circuits to implement transfer functions"
        },
        {
            "number": "LO12.",
            "description": "Be able to collaboratively work for design and implement filters"
        }
    ],
