        },
        {
            "number": "LO6.",
            "description": "Work as a team to understand actions that can be undertaken to limit an infectious disease pandemic"
        },
        {
            "number": "LO7.",
            "description": "Synthesise information and apply to a future scenario for public health in the post-pandemic era"
        }
