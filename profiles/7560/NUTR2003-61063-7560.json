    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Have a broad understanding of the importance of nutrition in population  health."
        },
        {
            "number": "LO2.",
            "description": "Describe the changing nutritional needs throughout the lifespan, including for infancy, childhood, adolescence, adult, maternal- pregnancy and lactation, and elderly."
        },
        {
            "number": "LO3.",
            "description": "Identify and describe the basis for dietary requirements, and major nutritional problems during each stage of the lifespan."
        },
        {
            "number": "LO4.",
            "description": "Critically  analyse fads and trends at various stages in the lifespan that may influence nutritional status."
        },
        {
            "number": "LO5.",
            "description": "Demonstrate a wide understanding of factors which can affect nutritional requirements and vulnerabilities that may affect people belonging to different cultural, socioeconomic and other special groups."
        },
        {
            "number": "LO6.",
            "description": "Understand the appropriate use of dietary and anthropometric methods to assess the nutritional status of individuals."
        },
        {
            "number": "LO7.",
            "description": "Critically analyse how current issues in nutrition or the food supply could have an impact on different stages of the lifespan."
        },
        {
            "number": "LO8.",
            "description": "Search, locate and identify appropriate resources  and demonstrate the ability to critically  analyse these resources."
        },
        {
            "number": "LO9.",
            "description": "Demonstrate an ability to communicate your findings effectively in written and oral formats."
        }
    ],
