    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Recognise situations where machine learning methods can offer advantages over traditional statistical modelling approaches to data analyses in health application."
        },
        {
            "number": "LO2.",
            "description": "Recognise and explain the differences between the goals of description and prediction."
        },
        {
            "number": "LO3.",
            "description": "Determine and implement appropriate machine learning approaches for description and prediction in real-world health applications."
        },
        {
            "number": "LO4.",
            "description": "Measure and explain the uncertainty of the results of analyses using machine learning approaches."
        },
        {
            "number": "LO5.",
            "description": "Interpret the results of analyses using machine learning in light of the assumptions required, the quality of input data, and the sensitivity to the specific technique implemented."
        },
        {
            "number": "LO6.",
            "description": "Critically appraise published papers concerning machine learning applications for classification on prediction in health."
        },
        {
            "number": "LO7.",
            "description": "Effectively communicate results of analyses in language suitable for a clinical or epidemiological journal."
        }
    ],
