        },
        {
            "number": "LO4.",
            "description": "demonstrate the fundamentals of critical thinking in psychology"
        },
        {
            "number": "LO5.",
            "description": "demonstrate further development of written and oral communication skills and effectively engage with others to discuss and evaluate empirical research"
        },
        {
            "number": "LO6.",
            "description": "demonstrate an awareness of ethical considerations when conducting psychological research"
        }
