    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Understand and be familiar in the use of the terminology used within cutaneous oncology;"
        },
        {
            "number": "LO2.",
            "description": "Have a clear understanding of the anatomy, histology and physiology of skin;"
        },
        {
            "number": "LO3.",
            "description": "Have a clear understanding of the biology and pathology of skin malignancies;"
        },
        {
            "number": "LO4.",
            "description": "Have a clear understanding of the risk factors for skin malignancies and precursor conditions and of core therapeutic principles;"
        },
        {
            "number": "LO5.",
            "description": "Have a clear understanding of the epidemiology of skin malignancies and precursor conditions, and the recommended prevention strategies;"
        },
        {
            "number": "LO6.",
            "description": "Demonstrate knowledge of relevant ethical and legal requirements and expectations;"
        },
        {
            "number": "LO7.",
            "description": "Display professional characteristics of a modern medical practitioner as expected by state medical boards such as working with patients in a therapeutic relationship and working as a member of a health care team;"
        },
        {
            "number": "LO8.",
            "description": "Understand basics of clinical, dermatoscopic and dermatopathological characteristics of skin lesions,  and how they correlate together and integrate with clinical practice;"
        },
        {
            "number": "LO9.",
            "description": "Understand basic research principles as they relate to critical evaluation of published medical literature."
        }
    ],
