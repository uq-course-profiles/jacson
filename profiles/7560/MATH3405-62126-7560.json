    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "describe and compute fundamental properties of a curve, such as curvature and torsion"
        },
        {
            "number": "LO2.",
            "description": "understand, calculate and use properties such as first and second fundamental form of surfaces in 3-space"
        },
        {
            "number": "LO3.",
            "description": "understand various ways in which calculus on flat spaces can be extended to calculus on curved spaces"
        },
        {
            "number": "LO4.",
            "description": "compute the mean and Gauss curvature of a surface in 3-space"
        },
        {
            "number": "LO5.",
            "description": "both describe geometrically what it means to differentiate along a surface and be able to compute this; in particular, to find geodesics on a given surface"
        },
        {
            "number": "LO6.",
            "description": "give examples of special types of surfaces, such as minimal surfaces, and analyse their properties"
        },
        {
            "number": "LO7.",
            "description": "understand and use major theorems such as the Gauss-Bonnet theorem, that describe the global geometry of a surface in terms of its topology"
        },
        {
            "number": "LO8.",
            "description": "define and compute integrals over manifolds"
        }
    ],
