    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "Explain the central role of sigma-algebras and measure in integration theory"
        },
        {
            "number": "LO2.",
            "description": "Deal with measurable functions and describe their importance to the definition of the integral"
        },
        {
            "number": "LO3.",
            "description": "Evaluate and use special measures on Euclidean spaces (e.g., Lebesgue measure)"
        },
        {
            "number": "LO4.",
            "description": "Decompose measures and explain the role this decomposition plays in the Radon-Nikodym theorem"
        },
        {
            "number": "LO5.",
            "description": "Discuss and interpret function spaces and modes of convergence"
        },
        {
            "number": "LO6.",
            "description": "Evaluate and apply integrals on product spaces using the relationship with repeated integrals"
        },
        {
            "number": "LO7.",
            "description": "Apply results from measure theory to other areas of mathematics"
        }
    ],
